.table {
    border: 0rpx solid darkgray;
}

.tr {
    display: flex;
    width: 100%;
    justify-content: center;
    align-items: center;
    border: 1rpx solid darkgray;
}

.td {
    width: 40%;
    padding: 10rpx;
    justify-content: center;
    text-align: center;
    /* border-left: 1rpx solid darkgray; */
    border-right: 1rpx solid darkgray;
}

.td-1 {
    width: 8.33%;
}

.td-2 {
    width: 16.6%;
}

.td-3 {
    width: 25%;
}

.td-4 {
    width: 33.33%;
}

.td-5 {
    width: 41.66%;
}

.td-6 {
    width: 50%;
}

.td-7 {
    width: 58.33%;
}

.td-8 {
    width: 66.66%;
}

.td-9 {
    width: 75%;
}

.td-10 {
    width: 83.33%;
}

.td-11 {
    width: 91.66%;
}

.td-12 {
    width: 100%;
}

.bg-w {
    background: #fff;
}

.bg-g {
    background: #e6f3f9;
}

.th {
    width: 40%;
    justify-content: center;
    background: #36f;
    color: #fff;
    display: flex;
    height: 3rem;
    align-items: center;
}
