<!--pages/register/register.wxml-->
<view class='pre-warp'>
  <image src="../../resource/images/img_login_bg.png" class='pre-image-bg'></image>
  <view class='pre-content'>
    <view class='pre-form'>
      <view class="pre-form-cell">
        <label>注册名称：</label>
        <input class="" name="input" placeholder-class="phcolor" placeholder="请输入用户名" />
      </view>
      <view class="pre-form-cell">
        <text decode="true" space="true">密&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;码：</text>
        <input class="" name="input" placeholder-class="phcolor" placeholder="请输入密码" />
      </view>
      <view class="pre-form-cell">
        <label>确认密码：</label>
        <input class="" name="input" placeholder-class="phcolor" placeholder="请确认密码" />
      </view>
      <view class="pre-form-cell">
        <text decode="true" space="true">联&nbsp;系&nbsp;人&nbsp;：</text>
        <input class="" name="input" placeholder-class="phcolor" placeholder="请输入联系人" />
      </view>
      <view class="pre-form-cell">
        <text decode="true" space="true">手&nbsp;机&nbsp;号&nbsp;：</text>
        <input class="" name="input" placeholder-class="phcolor" placeholder="请输入手机号" />
      </view>
      <view class="pre-form-cell">
        <text decode="true" space="true">验&nbsp;证&nbsp;码&nbsp;：</text>
        <input class="" name="input" placeholder-class="phcolor" placeholder="请输入验证码" />
      </view>
    </view>
    <label class='pre-chk-save'>
      <checkbox value="savePwd" /> 阅读并接受《会员协议》
    </label>
    <button>注册</button>
  </view>
</view>