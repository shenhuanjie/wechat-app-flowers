/* preImageList.wxss */
@import "../../resource/styles/preImageList.wxss";
/**index.wxss**/

swiper {
  height: 460rpx;
  width: 100%;
}

.slide-image {
  display: block;
  width: 100%;
  height: 460rpx;
}

.pre-block {
  margin: 45rpx 30rpx;
  min-height: 48rpx;
}

.pre-block .pre-block-title {
  float: left;
  font-size: 48rpx;
  line-height: 1;
}

.pre-block .pre-block-more {
  float: right;
  font-size: 24rpx;
  line-height: 2;
  color: #b0b0b0;
}