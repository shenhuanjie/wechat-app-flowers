<!--pages/products/products.wxml-->
<!-- 顶部搜索框 -->
<view class='pre-block page_row'>
  <!-- 顶部搜索框 -->
  <view class="search" style='display:flex;'>
    <view class="search_arr" style='flex:1'>
      <icon class="search_icon" size='20' type='search'></icon>
      <input class="" placeholder-class='phcolor' value="{{searchValue}}" placeholder='' />
    </view>
    <view style='width:48rpx;margin:4rpx;' bindtap='toggleItemList'>
      <icon class="iconfont {{iconlist}}"></icon>
    </view>
  </view>
</view>